{
    "name": "olcs/olcs-transfer",
    "description": "OLCS Transfer",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://devrepo.shd.ci.nonprod.dvsa.aws",
            "type": "composer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0.0",
        "mockery/mockery": "~0.9.3",
        "doctrine/annotations": "^1.2.3"
    },
    "config": {
        "preferred-install": "dist",
        "secure-http" : false
    },
    "autoload": {
        "psr-4": {
            "Dvsa\\Olcs\\Transfer\\": "src/"
        }
    }
}
